{
  "name": "vue3-starter",
  "version": "1.0.0",
  "author": {
    "name": "ydfk",
    "email": "lyh6728326@gmail.com",
    "url": "https://github.com/ydfk/vue3-starter"
  },
  "scripts": {
    "dev:pre": "node ./node_modules/esbuild/install.js",
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "type:check": "vue-tsc --noEmit --skipLibCheck",
    "clean:cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "clean:lib": "rimraf node_modules",
    "lint:eslint": "eslint --cache --max-warnings 0  \"{src,mock}/**/*.{vue,ts,tsx}\" --fix --max-warnings=500",
    "lint:prettier": "prettier --write  \"src/**/*.{js,json,tsx,css,less,scss,sass,vue,html,md}\"",
    "lint:stylelint": "stylelint --fix \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint": "pnpm lint:prettier && pnpm lint:stylelint && pnpm lint:eslint",
    "reinstall": "rimraf yarn.lock && rimraf package.lock.json && rimraf pnpm-lock.yaml && rimraf node_modules && pnpm install",
    "release": "standard-version",
    "postinstall": "patch-package",
    "analysis": "windicss-analysis",
    "cz": "git add . && git cz",
    "prepare": "husky install"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.1.0",
    "@vueuse/core": "^9.6.0",
    "ant-design-vue": "^3.2.15",
    "axios": "^1.2.0",
    "dayjs": "^1.11.6",
    "echarts": "^5.4.0",
    "intro.js": "^6.0.0",
    "lodash-es": "^4.17.21",
    "mitt": "^3.0.0",
    "mockjs": "^1.1.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.25",
    "pinia-plugin-persistedstate": "^2.4.0",
    "qs": "^6.11.0",
    "v-calendar": "3.0.0-alpha.8",
    "vue": "^3.2.45",
    "vue-echarts": "^6.2.3",
    "vue-router": "4"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.11.9",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vitejs/plugin-vue-jsx": "^2.1.1",
    "@vue/compiler-sfc": "^3.2.45",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-define-config": "^1.12.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.7.0",
    "less": "^4.1.3",
    "patch-package": "^6.5.0",
    "postcss": "^8.4.19",
    "postcss-html": "^1.5.0",
    "postcss-scss": "^4.0.5",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.8.0",
    "rimraf": "^3.0.2",
    "rollup": "2.79.1",
    "sass": "^1.56.1",
    "scss": "^0.2.4",
    "stylelint": "^14.15.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "unplugin-auto-import": "^0.11.5",
    "unplugin-vue-components": "^0.22.11",
    "vite": "^3.2.4",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-windicss": "^1.8.8",
    "vue-eslint-parser": "^9.1.0",
    "vue-tsc": "^1.0.9",
    "windicss": "^3.5.6",
    "windicss-analysis": "^0.3.5"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  }
}

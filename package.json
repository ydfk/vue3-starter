{
  "name": "vue3-starter",
  "version": "1.0.0",
  "author": {
    "name": "ydfk",
    "email": "lyh6728326@gmail.com",
    "url": "https://github.com/ydfk/vue3-starter"
  },
  "scripts": {
    "dev:pre": "node ./node_modules/esbuild/install.js",
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "type:check": "vue-tsc --noEmit --skipLibCheck",
    "clean:cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "clean:lib": "rimraf node_modules",
    "lint:eslint": "eslint --cache --max-warnings 0  \"{src,mock}/**/*.{vue,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write  \"src/**/*.{js,json,tsx,css,less,scss,sass,vue,html,md}\"",
    "lint:stylelint": "stylelint --cache --fix \"src/**/*.{vue,less,postcss,css,scss,sass}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint": "lint-staged -c ./.husky/lintstagedrc.js",
    "cz": "git add . && git cz",
    "prepare": "husky install",
    "reinstall": "rimraf yarn.lock && rimraf package.lock.json && rimraf node_modules && yarn install",
    "release": "standard-version",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "ant-design-vue": "^3.1.1",
    "axios": "^0.26.1",
    "dayjs": "^1.11.0",
    "lodash-es": "^4.17.21",
    "mockjs": "^1.1.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.13",
    "qs": "^6.10.3",
    "vue": "^3.2.31",
    "vue-router": "4"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^17.0.23",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vitejs/plugin-vue-jsx": "^1.3.9",
    "@vue/compiler-sfc": "^3.2.31",
    "autoprefixer": "^10.4.4",
    "commitizen": "^4.2.4",
    "commitlint-config-gitmoji": "^2.2.5",
    "commitlint-plugin-gitmoji": "^2.2.0",
    "conventional-changelog-gitmoji-config": "^1.4.3",
    "cz-customizable": "^6.3.0",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-define-config": "^1.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.6.0",
    "husky": "^7.0.4",
    "less": "^4.1.2",
    "lint-staged": "^12.3.7",
    "patch-package": "^6.4.7",
    "postcss": "^8.4.12",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "sass": "^1.50.0",
    "scss": "^0.2.4",
    "standard-version": "^9.3.2",
    "stylelint": "^14.6.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-order": "^5.0.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3",
    "unplugin-auto-import": "^0.6.9",
    "unplugin-vue-components": "^0.18.5",
    "vite": "^2.9.1",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-windicss": "^1.8.3",
    "vue-eslint-parser": "^8.3.0",
    "vue-tsc": "^0.33.9",
    "windicss": "^3.5.1",
    "windicss-analysis": "^0.3.5"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  }
}
